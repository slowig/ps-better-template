{
  "name": "dev",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "vue-cli-service build --no-clean",
    "lint": "vue-cli-service lint --fix",
    "dev": "vue-cli-service build --no-clean --mode development --watch",
    "test": "vue-cli-service test:unit --env=jest-environment-jsdom-sixteen",
    "vue": "vue-cli-service"
  },
  "dependencies": {
    "@vue/cli-plugin-unit-jest": "^4.5.12",
    "core-js": "^3.6.5",
    "vue": "^2.6.11",
    "vue-jest": "^3.0.7"
  },
  "devDependencies": {
    "@babel/core": "^7.13.15",
    "@babel/preset-env": "^7.13.15",
    "@testing-library/vue": "^5.6.2",
    "@vue/cli-plugin-babel": "^4.5.0",
    "@vue/cli-plugin-eslint": "^4.5.0",
    "@vue/cli-service": "^4.5.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.6.3",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^6.2.2",
    "jest-environment-jsdom-sixteen": "^1.0.3",
    "jsdom": "^16.5.3",
    "vue-template-compiler": "^2.6.11"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "ignorePatterns": [
      "*.vue"
    ],
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {}
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ],
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "vue"
    ],
    "transform": {
      ".*\\.(vue)$": "vue-jest",
      ".*\\.(js)$": "babel-jest"
    }
  }
}
